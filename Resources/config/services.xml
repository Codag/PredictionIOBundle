<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="predictionio.app_key">null</parameter>
        <parameter key="predictionio.service.class">Codag\PredictionIOBundle\Api\PredictionIO</parameter>
    </parameters>

    <services>
        <service id="predictionio" class="%predictionio.service.class%">
            <argument>%predictionio.app_key%</argument>
        </service>
    </services>

</container>
